{
  "require": {
    "php": "^7.4 || ^8.0",
    "lokielse/omnipay-alipay": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Wenprise\\Alipay\\": "src/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "extra": {
    "strauss": {
      "target_directory": "build",
      "namespace_prefix": "\\Wenprise\\Alipay\\",
      "classmap_prefix": "Wenprise_Alipay_",
      "constant_prefix": "WENPRISE_ALIPAY_",
      "packages": [
      ],
      "update_call_sites": false,
      "override_autoload": {
      },
      "exclude_from_copy": {
        "packages": [
        ],
        "namespaces": [
        ],
        "file_patterns": [
        ]
      },
      "exclude_from_prefix": {
        "packages": [
        ],
        "namespaces": [
        ],
        "file_patterns": [
        ]
      },
      "namespace_replacement_patterns": {
      },
      "delete_vendor_packages": true,
      "delete_vendor_files": true
    }
  },
  "scripts": {
    "prefix-namespaces": [
      "@php strauss.phar"
    ],
    "post-install-cmd": [
      "@prefix-namespaces",
      "composer dump-autoload -o"
    ],
    "post-update-cmd": [
      "@prefix-namespaces",
      "composer dump-autoload -o"
    ]
  }
}
